defaults
        log     global
        mode    http
        option  httplog
        option  dontlognull
        timeout connect 5000
        timeout client  50000
        timeout server  50000
        errorfile 400 /etc/haproxy/errors/400.http
        errorfile 403 /etc/haproxy/errors/403.http
        errorfile 408 /etc/haproxy/errors/408.http
        errorfile 500 /etc/haproxy/errors/500.http
        errorfile 502 /etc/haproxy/errors/502.http
        errorfile 503 /etc/haproxy/errors/503.http
        errorfile 504 /etc/haproxy/errors/504.http

#--beta-scribbles.tech-params-begin--
backend beta-scribbles.tech-backend
        balance roundrobin
        server 107965-web-01 52.91.132.17:80 check
        server 107965-web-02 334.224.63.71:80 check
frontend beta-scribbles.tech-frontend
        bind *:80
        mode http
        default_backend beta-scribbles.tech-backend
#--beta-scribbles.tech-params-end--


#--webdagi.tech-params-begin--
backend webdagi.tech-backend
        balance roundrobin
        server 107965-web-01 52.91.132.17:80 check
        server 107965-web-01 34.224.63.71:80 check
frontend webdagi.tech-frontend
        bind *:80
        mode http
        default_backend webdagi.tech-backend
#--webdagi.tech-params-end--
